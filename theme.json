        },
        {
          "slug": "gray-100",
          "color": "#f1f5f9",
          "name": "Gray 100"
        },
        {
          "slug": "gray-200",
          "color": "#e2e8f0",
          "name": "Gray 200"
        },
        {
          "slug": "gray-300",
          "color": "#cbd5e1",
          "name": "Gray 300"
        },
        {
          "slug": "gray-400",
          "color": "#94a3b8",
          "name": "Gray 400"
        },
        {
          "slug": "gray-500",
          "color": "#64748b",
          "name": "Gray 500"
        },
        {
          "slug": "gray-600",
          "color": "#475569",
          "name": "Gray 600"
        },
        {
          "slug": "gray-700",
          "color": "#334155",
          "name": "Gray 700"
        },
        {
          "slug": "gray-800",
          "color": "#1e293b",
          "name": "Gray 800"
        },
        {
          "slug": "gray-900",
          "color": "#0f172a",
          "name": "Gray 900"
        },
        {
          "slug": "success",
          "color": "#10b981",
          "name": "Success"
        },
        {
          "slug": "warning",
          "color": "#f59e0b",
          "name": "Warning"
        },
        {
          "slug": "error",
          "color": "#ef4444",
          "name": "Error"
        },
        {
          "slug": "info",
          "color": "#06b6d4",
          "name": "Info"
        }
      ],
      "gradients": [
        {
          "slug": "primary-gradient",
          "gradient": "linear-gradient(135deg, #2563eb 0%, #1d4ed8 100%)",
          "name": "Primary Gradient"
        },
        {
          "slug": "secondary-gradient",
          "gradient": "linear-gradient(135deg, #64748b 0%, #475569 100%)",
          "name": "Secondary Gradient"
        },
        {
          "slug": "warm-gradient",
          "gradient": "linear-gradient(135deg, #f59e0b 0%, #ef4444 100%)",
          "name": "Warm Gradient"
        },
        {
          "slug": "cool-gradient",
          "gradient": "linear-gradient(135deg, #06b6d4 0%, #2563eb 100%)",
          "name": "Cool Gradient"
        }
      ],
      "duotone": [
        {
          "colors": ["#2563eb", "#ffffff"],
          "slug": "primary-white",
          "name": "Primary and White"
        },
        {
          "colors": ["#64748b", "#ffffff"],
          "slug": "secondary-white",
          "name": "Secondary and White"
        }
      ]
    },
    "typography": {
      "customFontSize": true,
      "lineHeight": true,
      "dropCap": false,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "textDecoration": true,
      "textTransform": true,
      "fontFamilies": [
        {
          "fontFamily": "\"Inter\", system-ui, -apple-system, sans-serif",
          "slug": "primary",
          "name": "Primary (Inter)"
        },
        {
          "fontFamily": "\"Merriweather\", Georgia, serif",
          "slug": "secondary",
          "name": "Secondary (Merriweather)"
        },
        {
          "fontFamily": "\"JetBrains Mono\", \"Fira Code\", monospace",
          "slug": "monospace",
          "name": "Monospace"
        }
      ],
      "fontSizes": [
        {
          "size": "0.75rem",
          "slug": "x-small",
          "name": "Extra Small"
        },
        {
          "size": "0.875rem",
          "slug": "small",
          "name": "Small"
        },
        {
          "size": "1rem",
          "slug": "medium",
          "name": "Medium"
        },
        {
          "size": "1.125rem",
          "slug": "large",
          "name": "Large"
        },
        {
          "size": "1.25rem",
          "slug": "x-large",
          "name": "Extra Large"
        },
        {
          "size": "1.5rem",
          "slug": "xx-large",
          "name": "2X Large"
        },
        {
          "size": "1.875rem",
          "slug": "xxx-large",
          "name": "3X Large"
        },
        {
          "size": "2.25rem",
          "slug": "xxxx-large",
          "name": "4X Large"
        },
        {
          "size": "3rem",
          "slug": "xxxxx-large",
          "name": "5X Large"
        }
      ]
    },
    "spacing": {
      "custom": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"],
      "padding": true,
      "margin": true,
      "spacingScale": {
        "operator": "*",
        "increment": 1.5,
        "steps": 7,
        "mediumStep": 1.5,
        "unit": "rem"
      },
      "spacingSizes": [
        {
          "size": "0",
          "slug": "0",
          "name": "0"
        },
        {
          "size": "0.25rem",
          "slug": "10",
          "name": "1"
        },
        {
          "size": "0.5rem",
          "slug": "20",
          "name": "2"
        },
        {
          "size": "0.75rem",
          "slug": "30",
          "name": "3"
        },
        {
          "size": "1rem",
          "slug": "40",
          "name": "4"
        },
        {
          "size": "1.25rem",
          "slug": "50",
          "name": "5"
        },
        {
          "size": "1.5rem",
          "slug": "60",
          "name": "6"
        },
        {
          "size": "2rem",
          "slug": "70",
          "name": "7"
        },
        {
          "size": "2.5rem",
          "slug": "80",
          "name": "8"
        },
        {
          "size": "3rem",
          "slug": "90",
          "name": "9"
        },
        {
          "size": "4rem",
          "slug": "100",
          "name": "10"
        },
        {
          "size": "5rem",
          "slug": "110",
          "name": "11"
        },
        {
          "size": "6rem",
          "slug": "120",
          "name": "12"
        }
      ]
    },
    "border": {
      "custom": true,
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "shadow": {
      "custom": true,
      "defaultPresets": false,
      "presets": [
        {
          "slug": "sm",
          "shadow": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
          "name": "Small"
        },
        {
          "slug": "md",
          "shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1)",
          "name": "Medium"
        },
        {
          "slug": "lg",
          "shadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1)",
          "name": "Large"
        },
        {
          "slug": "xl",
          "shadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1)",
          "name": "Extra Large"
        }
      ]
    },
    "layout": {
      "contentSize": "768px",
      "wideSize": "1280px"
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": true
        },
        "color": {
          "background": true,
          "text": true
        },
        "spacing": {
          "padding": true
        },
        "typography": {
          "fontSize": true,
          "fontWeight": true
        }
      },
      "core/heading": {
        "color": {
          "background": true,
          "text": true
        },
        "typography": {
          "fontSize": true,
          "fontWeight": true,
          "lineHeight": true
        }
      },
      "core/paragraph": {
        "color": {
          "background": true,
          "text": true
        },
        "typography": {
          "fontSize": true,
          "lineHeight": true
        }
      },
      "core/quote": {
        "border": {
          "color": true,
          "style": true,
          "width": true
        },
        "color": {
          "background": true,
          "text": true
        },
        "spacing": {
          "padding": true
        },
        "typography": {
          "fontSize": true,
          "fontStyle": true
        }
      },
      "core/cover": {
        "border": {
          "radius": true
        },
        "spacing": {
          "padding": true
        }
      },
      "core/group": {
        "border": {
          "color": true,
          "radius": true,
          "style": true,
          "width": true
        },
        "color": {
          "background": true,
          "text": true
        },
        "spacing": {
          "padding": true,
          "margin": true
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--white)",
      "text": "var(--wp--preset--color--gray-900)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.5"
    },
    "spacing": {
      "padding": {
        "top": "0",
        "right": "var(--wp--preset--spacing--40)",
        "bottom": "0",
        "left": "var(--wp--preset--spacing--40)"
      }
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary-dark)"
          }
        },
        ":focus": {
          "color": {
            "text": "var(--wp--preset--color--primary-dark)"
          }
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xxxx-large)",
          "fontWeight": "700",
          "lineHeight": "1.25"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--60)"
          }
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xxx-large)",
          "fontWeight": "700",
          "lineHeight": "1.25"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--50)"
          }
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "600",
          "lineHeight": "1.25"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--50)"
          }
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600",
          "lineHeight": "1.25"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--40)"
          }
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600",
          "lineHeight": "1.25"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--40)"
          }
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600",
          "lineHeight": "1.25"
        },
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--40)"
          }
        }
      },
      "button": {
        "border": {
          "radius": "0.375rem"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--white)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)",
            "right": "var(--wp--preset--spacing--60)",
            "bottom": "var(--wp--preset--spacing--30)",
            "left": "var(--wp--preset--spacing--60)"
          }
        },
        "typography": {
          "fontWeight": "600"
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--primary-dark)"
          }
        },
        ":focus": {
          "color": {
            "background": "var(--wp--preset--color--primary-dark)"
          }
        }
      }
    },
    "blocks": {
      "core/button": {
        "variations": {
          "outline": {
            "border": {
              "color": "var(--wp--preset--color--primary)",
              "style": "solid",
              "width": "2px"
            },
            "color": {
              "background": "transparent",
              "text": "var(--wp--preset--color--primary)"
            },
            ":hover": {
              "color": {
                "background": "var(--wp--preset--color--primary)",
                "text": "var(--wp--preset--color--white)"
              }
            }
          }
        }
      },
      "core/quote": {
        "border": {
          "color": "var(--wp--preset--color--primary)",
          "style": "solid",
          "width": "0 0 0 4px"
        },
        "spacing": {
          "padding": {
            "left": "var(--wp--preset--spacing--60)"
          }
        },
        "typography": {
          "fontStyle": "italic"
        }
      },
      "core/pullquote": {
        "border": {
          "color": "var(--wp--preset--color--primary)",
          "style": "solid",
          "width": "4px 0"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--60)",
            "bottom": "var(--wp--preset--spacing--60)"
          }
        }
      },
      "core/separator": {
        "color": {
          "text": "var(--wp--preset--color--gray-300)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "700"
        }
      }
    }
  },
  "customTemplates": [
    {
      "name": "page-fullwidth",
      "title": "Full Width Page",
      "postTypes": ["page"]
    },
    {
      "name": "page-landing",
      "title": "Landing Page",
      "postTypes": ["page"]
    },
    {
      "name": "single-portfolio",
      "title": "Portfolio Single",
      "postTypes": ["portfolio"]
    }
  ],
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}